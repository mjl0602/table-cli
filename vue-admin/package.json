{
  "name": "vue-element-admin",
  "version": "1.0.0",
  "description": "基于vue-element-admin的后台管理系统",
  "author": "fcheng<fchengjin@126.com>",
  "license": "MIT",
  "scripts": {
    "dev": "vue-cli-service serve",
    "build:prod": "vue-cli-service build",
    "build:sit": "vue-cli-service build --mode sit",
    "deploy:sit": "node upload.js",
    "build:preview": "node build/index.js --preview",
    "build:report": "node build/index.js --report",
    "lint": "eslint --ext .js,.vue src",
    "test": "npm run lint",
    "test:unit": "vue-cli-service test:unit",
    "svgo": "svgo -f src/icons/svg --config=src/icons/svgo.yml"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,vue}": [
      "eslint --fix",
      "git add"
    ]
  },
  "keywords": [
    "vue",
    "element-ui",
    "admin",
    "management-system",
    "admin-template"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PanJiaChen/vue-element-admin.git"
  },
  "dependencies": {
    "axios": "0.18.0",
    "clipboard": "1.7.1",
    "codemirror": "5.44.0",
    "dropzone": "5.5.1",
    "echarts": "4.1.0",
    "element-ui": "2.6.1",
    "file-saver": "2.0.1",
    "fuse.js": "3.4.4",
    "js-cookie": "2.2.0",
    "jszip": "3.2.0",
    "normalize.css": "7.0.0",
    "nprogress": "0.2.0",
    "path-to-regexp": "2.4.0",
    "screenfull": "4.0.1",
    "showdown": "1.9.0",
    "sortablejs": "1.8.3",
    "tui-editor": "1.3.2",
    "vue": "2.6.8",
    "vue-count-to": "1.0.13",
    "vue-router": "3.0.2",
    "vue-splitpane": "1.0.2",
    "vuedraggable": "2.17.0",
    "vuex": "3.1.0",
    "xlsx": "0.14.1"
  },
  "devDependencies": {
    "@babel/core": "7.0.0",
    "@babel/register": "7.0.0",
    "@vue/cli-plugin-babel": "3.5.0",
    "@vue/cli-plugin-unit-jest": "3.5.0",
    "@vue/cli-service": "3.5.0",
    "@vue/test-utils": "1.0.0-beta.29",
    "archiver": "^3.0.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.0.1",
    "babel-jest": "23.6.0",
    "chalk": "^2.4.2",
    "connect": "3.6.6",
    "eslint": "5.15.1",
    "eslint-plugin-vue": "5.2.2",
    "husky": "1.3.1",
    "less": "^3.9.0",
    "less-loader": "^5.0.0",
    "lint-staged": "7.2.2",
    "node-sass": "^4.9.0",
    "node-ssh": "^5.1.2",
    "ora": "^3.2.0",
    "runjs": "^4.3.2",
    "sass-loader": "^7.1.0",
    "script-ext-html-webpack-plugin": "2.1.3",
    "script-loader": "0.7.2",
    "serve-static": "^1.13.2",
    "svg-sprite-loader": "4.1.3",
    "svgo": "1.2.0",
    "vue-template-compiler": "2.6.8"
  },
  "engines": {
    "node": ">=8.9",
    "npm": ">= 3.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
